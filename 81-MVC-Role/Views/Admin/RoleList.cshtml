@using Microsoft.AspNetCore.Identity
@model IEnumerable<IdentityRole>
@{
    ViewData["Title"] = "RoleList";
}

<h1>Role List</h1>
<table class="table">
    <thead>
        <tr>
            <td>Rol Id</td>
            <td>Rol Adu</td>
            <td>Islemler</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <td>@role.Id</td>
                <td>@role.Name</td>
                <td>
                    <a href="#">Düzenle</a> | <a href="#">Sil</a>
                </td>
            </tr>
        }
    </tbody>
</table>

